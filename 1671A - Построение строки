#include <iostream>
#include <string>
int main(){
    int t,s;
    std:: cin >> t;
    std:: cin >> s;
    std:: string s1 = "aa" ;
    std:: string s2 = "aaa";
    std:: string s3 = "bb";
    std:: string s4 = "bbb";
    if (s = s1 || s2 || s3 || s4|| s1+s2|| s1+s3||s1+s4||s2+s3||s2+s4||s3+s4||s1+s2+s3||s1+s2+s4||s2+s3+s4||s1+s2+s3+s4){
        std:: cout << "Yes";
    }
    else {
        std:: cout << "NO";
    }
 
}
